{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"dist/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"dist/index.html"
        },
        {
            "src":"/about",
            "dest":"dist/index.html"
        },
        {
            "src":"/contact",
            "dest":"dist/index.html"
        },
        {
            "src":"/labs",
            "dest":"dist/index.html"
        },
        {
            "src":"/signup",
            "dest":"dist/index.html"
        },
        {
            "src":"/login",
            "dest":"dist/index.html"
        },
        {
            "src":"/account-verification",
            "dest":"dist/index.html"
        },
        {
            "src":"/complain",
            "dest":"dist/index.html"
        },
        {
            "src":"/labDescription",
            "dest":"dist/index.html"
        },
        {
            "src":"/updateProfile",
            "dest":"dist/index.html"
        },
        {
            "src":"/account",
            "dest":"dist/index.html"
        },
        {
            "src":"/changePassword",
            "dest":"dist/index.html"
        },
        {
            "src":"/forgetPassword",
            "dest":"dist/index.html"
        },
        {
            "src":"/reset-password",
            "dest":"dist/index.html"
        },
        {
            "src":"/checkout",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/complaints",
            "dest":"dist/index.html"
        },
        {
            "src":"/admin/users",
            "dest":"dist/index.html"
        },        
        {
            "src":"/admin/labs",
            "dest":"dist/index.html"
        },        
        {
            "src":"/admin/unauthorized",
            "dest":"dist/index.html"
        },        
        {
            "src":"/admin/workers",
            "dest":"dist/index.html"
        },
        {
            "src":"/*",
            "dest":"dist/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"dist/$1"
        }
    ]
}